pipeline {
      agent  any
      
      stages {
            stage(`Checkout`){
                  steps {
                        checkout scm
                  }
            }
            stage (`Restore Dependencies`){
                  steps {
                        bat `dotnet build`
                  }
            }
            stage (`Build`){
                  steps {
                        bat `dotnet build --no-restore`
                  }
            }
            stage (`Test`) {
                  steps {
                        bat `dotnet test --no-build --verbosity normal`
                  }
            }
      }

    
}